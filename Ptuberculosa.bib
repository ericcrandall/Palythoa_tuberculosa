  author = {Shinzato, Chuya and Khalturin, Konstantin and Inoue, Jun and Zayasu, Yuna and Kanda, Miyuki and Kawamitsu, Mayumi and Yoshioka, Yuki and Yamashita, Hiroshi and Suzuki, Go and Satoh, Noriyuki},
