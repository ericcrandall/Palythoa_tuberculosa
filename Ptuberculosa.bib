@article{cercaRemovingBadApples2021,
  title = {Removing the Bad Apples: {{A}} Simple Bioinformatic Method to Improve Loci-Recovery in de Novo {{RADseq}} Data for Non-Model Organisms},
  shorttitle = {Removing the Bad Apples},
  author = {Cerca, Jos{\'e} and Maurstad, Marius F. and Rochette, Nicolas C. and {Rivera-Col{\'o}n}, Angel G. and Rayamajhi, Niraj and Catchen, Julian M. and Struck, Torsten H.},
